{
  "name": "TabList",
  "version": "1.0.6",
  "description": "This extension will list all opened tabs in new tab. Useful if you have opened like hundred of tabs.",
  "chrome_url_overrides" : {
    "newtab": "newTab.html"
  },
  "icons": { "128": "icon.png" },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "chrome://favicon/"
  ],
  "options_page": "options.html",
  "manifest_version": 2,
  "web_accessible_resources": ["fonts/*"]
}
